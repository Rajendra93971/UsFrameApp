<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="UsFrameApp.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:view="clr-namespace:UsFrameApp.View"
	Shell.NavBarIsVisible="True">

	<ShellContent ContentTemplate="{DataTemplate view:SplashPage}" Route="SplashPage" />
	<ShellContent ContentTemplate="{DataTemplate view:HomePage}" Route="HomePage" />
	<ShellContent ContentTemplate="{DataTemplate view:RoomPage}" Route="RoomPage" />
	<ShellContent ContentTemplate="{DataTemplate view:ConnectingPage}" Route="ConnectingPage" />
	<ShellContent ContentTemplate="{DataTemplate view:SettingsPage}" Route="SettingsPage" />
</Shell>
